{
	"name": "nodemodules-cleaner",
	"private": true,
	"version": "0.0.3",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "tauri",
		"tauri:dev": "tauri dev",
		"tauri:build": "tauri build",
		"tauri:build:debug": "tauri build --debug",
		"tauri:build:windows": "tauri build --target x86_64-pc-windows-msvc",
		"tauri:build:macos": "tauri build --target x86_64-apple-darwin",
		"tauri:build:macos-arm": "tauri build --target aarch64-apple-darwin",
		"tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu",
		"setup": "bash scripts/setup.sh",
		"clean": "rm -rf dist src-tauri/target",
		"clean:all": "yarn clean && rm -rf node_modules"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.12",
		"@tauri-apps/api": "^2",
		"@tauri-apps/plugin-opener": "^2",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"tailwindcss": "^4.1.12"
	},
	"devDependencies": {
		"@tauri-apps/cli": "^2",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react": "^4.6.0",
		"typescript": "~5.8.3",
		"vite": "^7.0.4"
	}
}
